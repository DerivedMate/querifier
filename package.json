  "bugs": {
    "url": "https://github.com/LastShadowPL/querify/issues"
  },
  "homepage": "https://github.com/LastShadowPL/querify#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/jest": "^23.3.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "jest": "^23.5.0",
    "ts-jest": "^23.0.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.0.3",
